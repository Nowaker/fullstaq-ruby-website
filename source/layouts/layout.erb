<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <!-- Use the title from a page's frontmatter if it has one -->
    <title><%= current_page.data.title || "Fullstaq Ruby" %></title>
    <%= stylesheet_link_tag "bootstrap", "site", "custom" %>
    <%= javascript_include_tag "jquery-3.4.1.min", "bootstrap.min", "site" %>
    <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:400,300,600,400italic,700" rel="stylesheet" type="text/css">
  </head>
  <body>
    <header role="banner" id="fh5co-header">
      <div class="container">
        <nav class="navbar navbar-default">
          <div class="navbar-header">
            <!-- Mobile Toggle Menu Button -->
            <a href="#" class="js-fh5co-nav-toggle fh5co-nav-toggle" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar"><i></i></a>
            <a class="navbar-brand" href="index.html"><%= image_tag 'fullstaq-logo-small.png', srcset: 'fullstaq-logo-small.png 1x, fullstaq-logo-small@2x.png 2x', class: 'logo', alt: '', 'aria-hidden': true %><span class="vendor">Fullstaq</span> <span class="product">Ruby</span> <sup class="beta-disclaimer">beta</sup></a>
          </div>
          <div id="navbar" class="navbar-collapse collapse">
            <ul class="nav navbar-nav navbar-right">
              <% navbar_dropdown 'Get Started', :get_started, @current_section do %>
                <li><a href="#">For Servers</a></li>
                <li role="separator" class="divider"></li>
                <li class="disabled"><a href="#">For Containers <span class="badge">coming soon!</span></a></li>
                <li class="disabled"><a href="#">For Heroku <span class="badge">coming soon!</span></a></li>
              <% end %>
              <% navbar_dropdown 'Editions', :editions, @current_section do %>
                <li><a href="#">Server Edition</a></li>
                <li role="separator" class="divider"></li>
                <li class="disabled"><a href="#">Container Edition <span class="badge">coming soon!</span></a></li>
                <li class="disabled"><a href="#">Heroku Edition <span class="badge">coming soon!</span></a></li>
              <% end %>
              <%= navbar_link 'Community', :community, @current_section, '#' %>
              <%= navbar_link 'Blog', :blog, @current_section, '#' %>
              <%= navbar_link 'About', :about, @current_section, '#' %>
            </ul>
          </div>
        </nav>
      </div>
    </header>

    <%= yield %>
  </body>
</html>
